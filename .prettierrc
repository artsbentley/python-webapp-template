{
  "useTabs": true,
  "tabWidth": 1,
  "singleQuote": false,
  "printWidth": 150,
  "templateTokens": ["template-tag", "template-variable"],
  "preserveBlankLines": true,
  "plugins": ["prettier-plugin-jinja-template"],
  "jinjaTemplateBracketSpacing": true,
  "overrides": [
    {
      "files": "*.html",
      "options": {
        "parser": "jinja-template"
      }
    }
  ]
}
